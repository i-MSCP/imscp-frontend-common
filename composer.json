{
  "name": "imscp/imscp-frontend-common",
  "description": "Common module for i-MSCP FrontEnd",
  "license": "LGPL-2.1-only",
  "keywords": [
    "imscp",
    "frontend",
    "common"
  ],
  "homepage": "https://www.i-mscp.net/",
  "support": {
    "issues": "https://youtrack.i-mscp.net/",
    "source": "https://github.com/i-MSCP/imscp/"
  },
  "authors": [
    {
      "name": "Laurent Declercq",
      "email": "l.declercq@nuxwin.com",
      "homepage": "https://www.i-mscp.net/",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "doctrine/migrations": "^1.7",
    "doctrine/doctrine-orm-module": "^2.1.2",
    "zendframework/zend-cache": "^2.8.5",
    "zendframework/zend-code": "^3.3.0",
    "zendframework/zend-config": "^3.2.0",
    "zendframework/zend-escaper": "^2.6.0",
    "zendframework/zend-eventmanager": "^3.2.1",
    "zendframework/zend-hydrator": "^2.4.0",
    "zendframework/zend-http": "^2.8.0",
    "zendframework/zend-filter": "^2.8.0",
    "zendframework/zend-i18n": "^2.8.0",
    "zendframework/zend-inputfilter": "^2.8.1",
    "zendframework/zend-log": "^2.10.0",
    "zendframework/zend-mvc": "^3.1.1",
    "zendframework/zend-mvc-form": "^1.0.0",
    "zendframework/zend-mvc-i18n": "^1.1.0",
    "zendframework/zend-mvc-plugins": "^1.0.1",
    "zendframework/zend-paginator": "^2.8.1",
    "zendframework/zend-router": "^3.0.2",
    "zendframework/zend-session": "^2.8.5",
    "zendframework/zend-servicemanager": "^3.3.2",
    "zendframework/zend-stdlib": "^3.1.0",
    "zendframework/zend-uri": "^2.6.1",
    "zendframework/zend-validator": "^2.10.2",
    "zendframework/zend-view": "^2.10.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "iMSCP\\Frontend\\Common\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "zf": {
      "module": [
        "DoctrineORMModule",
        "DoctrineModule",
        "Zend\\Cache",
        "Zend\\Filter",
        "Zend\\Form",
        "Zend\\Hydrator",
        "Zend\\I18n",
        "Zend\\InputFilter",
        "Zend\\Mvc\\Plugin\\Prg",
        "Zend\\Mvc\\Plugin\\Identity",
        "Zend\\Mvc\\Plugin\\FlashMessenger",
        "Zend\\Mvc\\Plugin\\FilePrg",
        "Zend\\Mvc\\I18n",
        "Zend\\Paginator",
        "Zend\\Router",
        "Zend\\Session",
        "Zend\\Validator",
        "iMSCP\\Frontend\\Common"
      ]
    }
  },
  "scripts": {
    "check": [
      "@cs-check"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
