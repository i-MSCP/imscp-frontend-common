{
  "name": "imscp/imscp-frontend-common",
  "description": "Common module for i-MSCP FrontEnd",
  "license": "LGPL-2.1-only",
  "keywords": [
    "imscp",
    "frontend",
    "common"
  ],
  "homepage": "https://www.i-mscp.net/",
  "support": {
    "issues": "https://youtrack.i-mscp.net/",
    "source": "https://github.com/i-MSCP/imscp/"
  },
  "authors": [
    {
      "name": "Laurent Declercq",
      "email": "l.declercq@nuxwin.com",
      "homepage": "https://www.i-mscp.net/",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "doctrine/common": "^v2.8",
    "mso/idna-convert": "^v1.1",
    "pear/net_dns2": "^v1.4",
    "phpseclib/phpseclib": "^2.0",
    "zendframework/zend-cache": "^2.0",
    "zendframework/zend-component-installer": "^2.0",
    "zendframework/zend-db": "^2.0",
    "zendframework/zend-inputfilter": "^2.8",
    "zendframework/zend-json": "^3.0",
    "zendframework/zend-log": "^2.0",
    "zendframework/zend-mvc": "^3.0",
    "zendframework/zend-mvc-form": "^3.0",
    "zendframework/zend-mvc-i18n": "^1.0",
    "zendframework/zend-mvc-plugins": "^1.0",
    "zendframework/zend-session": "^2.0",
    "zendframework/zend-serializer": "^2.8"
  },
  "require-dev": {
    "aist/aist-git-tools": "^0.1.3",
    "andrey-mokhov/bjy-profiler": "^1.3",
    "jhuet/zdt-logger-module": "^1.0",
    "phpunit/phpunit": "^5.7.27 || ^6.5.8 || ^7.1.5",
    "san/san-session-toolbar": "^2.0",
    "zendframework/zend-mvc-console": "^1.2",
    "zendframework/zend-test": "^3.2",
    "zendframework/zend-coding-standard": "~1.0.0",
    "zendframework/zend-developer-tools": "^1.1.0",
    "zfcampus/zf-development-mode": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "iMSCP\\Frontend\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iMSCP\\Frontend\\Common\\": "/test"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "zf": {
      "module": "iMSCP\\Frontend\\Common"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  },
  "config": {
    "sort-packages": true,
    "classmap-authoritative": false,
    "discard-changes": true,
    "htaccess-protect": true,
    "optimize-autoloader": false,
    "apcu-autoloader": false,
    "preferred-install": "dist",
    "process-timeout": 5000
  }
}
